<div class="col-md-4 col-md-offset-4">
  <h2>编辑job</h2>
  <%= simple_form_for [:admin,@job] do |f | %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :wage_upper_bound,  :label => "薪资上限" %>
    <%= f.input :wage_lower_bound, :label => "薪资下限" %>
    <%= f.input :contact_email%>
    <%= f.input :is_hidden %>
    <%= f.submit "Submit", class: "btn btn-sm btn-primary"%>
  <% end %>
</div>
